<h1>{{title}}</h1>
<p>Create Client Account</p>
<div>
    <form class="form-horizontal" role="form" style="width: 50%;" method="post" action="/client-list-accounts">
        <div class="form-group">
            <label for="name" class="col-sm-2 control-label">Account Id</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="accountId" name="accountId" placeholder="AccountId" value="">
            </div>
        </div>
        <div class="form-group">
            <div class="col-sm-10 col-sm-offset-2">
                <input id="submit" name="submit" type="submit" value="Send" class="btn btn-primary">
            </div>
        </div>
    </form>
</div>
